<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<TitledPane id="mainComponent" fx:id="mainComponent" text="SYMBOL" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.stockmenu.StockMenuItemController">
   <content>
      <AnchorPane styleClass="pane-anchor">
         <children>
            <VBox prefWidth="130.0" spacing="5.0">
               <children>
                  <Label fx:id="stockInfoLabel" onMouseClicked="#requestStockInfo" text="View Stock's Info">
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Label>
                  <Label fx:id="newCommandLabel" onMouseClicked="#requestCommandInfo" text="Apply New Command">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Label>
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <padding>
                  <Insets left="5.0" top="3.0" />
               </padding>
            </VBox>
         </children></AnchorPane>
   </content>
</TitledPane>
